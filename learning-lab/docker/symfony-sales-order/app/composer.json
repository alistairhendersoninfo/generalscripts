{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/form": "^7.0",
        "symfony/security-bundle": "^7.0",
        "doctrine/orm": "^3.2",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/migrations": "^3.6"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0"
        }
    }
}
